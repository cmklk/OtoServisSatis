@model OtoServisSatis.Entities.Satis

@{
    ViewData["Title"] = "Delete";
}



<h5><strong class="text-danger">Uyarı!</strong>Satış bilgileri silinecektir.</h5>
<div>
 
    <dl class="row">
        
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SatisFiyati)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SatisFiyati)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SatisTarihi)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SatisTarihi)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Arac)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Arac.Model)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Musteri)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Musteri.Adi)  @Html.DisplayFor(model => model.Musteri.Soyadi)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Sil" class="btn btn-danger my-2" /> |
        <a asp-action="Index" class="my-2 btn btn-primary">Geri Dön</a>
    </form>
</div>
